{
  "kind": "collectionType",
  "collectionName": "barbers_stores",
  "info": {
    "singularName": "barbers-store",
    "pluralName": "barbers-stores",
    "displayName": "BarbersStore",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "StoreName": {
      "type": "string"
    },
    "StoreLocation": {
      "type": "string"
    },
    "StoreImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "barber_services": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::store-service.store-service",
      "inversedBy": "owning_barber"
    },
    "members": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::barber-store-member.barber-store-member",
      "mappedBy": "barbers_store"
    },
    "bserv": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::bserv.bserv",
      "mappedBy": "barbers_store"
    },
    "snd": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
