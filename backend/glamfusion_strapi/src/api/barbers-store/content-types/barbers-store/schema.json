{
  "kind": "collectionType",
  "collectionName": "barbers_stores",
  "info": {
    "singularName": "barbers-store",
    "pluralName": "barbers-stores",
    "displayName": "BarbersStore"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "StoreName": {
      "type": "string"
    },
    "Location": {
      "type": "string"
    },
    "StoreImage": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "barber_services": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::store-service.store-service",
      "inversedBy": "owning_barber"
    }
  }
}
