{
  "kind": "collectionType",
  "collectionName": "make_up_stores",
  "info": {
    "singularName": "make-up-store",
    "pluralName": "make-up-stores",
    "displayName": "MakeUpStore",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "StoreName": {
      "type": "string"
    },
    "StoreLocation": {
      "type": "string"
    },
    "StoreImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "make_up_services": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::make-up-service.make-up-service",
      "inversedBy": "owning_artists"
    },
    "members": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::make-up-store-member.make-up-store-member",
      "mappedBy": "make_up_store"
    }
  }
}
