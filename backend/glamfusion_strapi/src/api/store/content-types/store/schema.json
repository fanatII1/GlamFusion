{
  "kind": "collectionType",
  "collectionName": "stores",
  "info": {
    "singularName": "store",
    "pluralName": "stores",
    "displayName": "BarberStore",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "StoreName": {
      "type": "string"
    },
    "Location": {
      "type": "text"
    },
    "StoreBuildingImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "barber_services": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::store-service.store-service",
      "inversedBy": "owning_barbers"
    }
  }
}
