{
  "kind": "collectionType",
  "collectionName": "store_services",
  "info": {
    "singularName": "store-service",
    "pluralName": "store-services",
    "displayName": "BarberService",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "ServiceName": {
      "type": "string",
      "required": true
    },
    "ServiceImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "owning_braiders": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::braiding-store.braiding-store",
      "mappedBy": "braiding_services"
    },
    "ServicePrice": {
      "type": "decimal"
    },
    "owning_barber": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::barbers-store.barbers-store",
      "mappedBy": "barber_services"
    }
  }
}
