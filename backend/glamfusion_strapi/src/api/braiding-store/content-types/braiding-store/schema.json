{
  "kind": "collectionType",
  "collectionName": "braiding_stores",
  "info": {
    "singularName": "braiding-store",
    "pluralName": "braiding-stores",
    "displayName": "BraidingStore",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "StoreName": {
      "type": "string"
    },
    "StoreLocation": {
      "type": "string"
    },
    "StoreImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "braiding_services": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::braiding-service.braiding-service",
      "inversedBy": "owning_braiders"
    },
    "members": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::braiding-store-member.braiding-store-member",
      "mappedBy": "braiding_store"
    }
  }
}
